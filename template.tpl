___SANDBOXED_JS_FOR_SERVER___

const encodeUriComponent = require('encodeUriComponent');
const getCookieValues = require('getCookieValues');
const getEventData = require('getEventData');
const getRequestHeader = require('getRequestHeader');
const makeString = require('makeString');
const sendHttpGet = require('sendHttpGet');
const setCookie = require('setCookie');
const parseUrl = require('parseUrl');

/*
 * Build query string with object properties
 * @param {Object} requestParameters
 * @return string
 */
const buildQueryString = (requestParameters) => {
  let queryString = [];

  for (const parameter in requestParameters) {
    if (requestParameters[parameter]) {
      queryString.push(parameter + '=' + encodeUriComponent(requestParameters[parameter]));
    }
  }

  return queryString.join('&');
};


/**
 * Perform main action based on mode
 */
if (data.mode == 'capture') {
  // Capture lead, set tracking cookie
  const pageLocation = getEventData('page_location') || getRequestHeader('referer');

  // Check whether we have a page location
  if (pageLocation) {
    const urlObject = parseUrl(pageLocation);
    const userId = urlObject.searchParams.kzcid;

    // Check if we have a user ID (kzcid set)
    if (userId) {
      // Set cookie to use later
      setCookie('_kzcid', makeString(userId), {
        'max-age': 3600 * 24 * 365, // one year
        domain: data.cookieDomain,
        path: '/',
        httpOnly: false,
        secure: true,
      });
    }
  }
  
  data.gtmOnSuccess();
} else if (data.mode == 'conversion') {
  // Record a conversion
  
  // Check if we have an user ID
  const userId = getCookieValues('_kzcid')[0];

  // Check if cookie is set:
  if (typeof userId == 'undefined' || userId == null) {
    // No cookie, do not record conversion
    data.gtmOnSuccess();
    return;
  }

  const pageLocation = getEventData('page_location') || getRequestHeader('referer');

  // Send conversion information to Keuze.nl
  const url = 'https://gtm-tracking.keuze.nl/v1/conversion?' +
    buildQueryString({ partner: data.partner, url: pageLocation, uid: userId, product: data.product, amount: data.amount });
  
  sendHttpGet(url).then((result) => {
    if (result.statusCode >= 200 && result.statusCode < 300) {
      data.gtmOnSuccess();
    } else {
      data.gtmOnFailure();
    }
  });
}


